{
  "debug.allowBreakpointsEverywhere": true,
  "editor.accessibilitySupport": "off",
  "editor.codeLens": true,
  "editor.colorDecorators": true,
  "editor.dragAndDrop": false,
  "editor.folding": false,
  "editor.fontLigatures": true,
  "editor.glyphMargin": true,
  "editor.minimap.enabled": false,
  "editor.renderWhitespace": "selection",
  "explorer.compactFolders": true,
  "editor.scrollBeyondLastLine": false,
  "editor.snippetSuggestions": "none",
  "editor.cursorSurroundingLines": 3,
  "editor.useTabStops": true,
  "editor.formatOnPaste": false,
  "editor.tabCompletion": "on",
  "editor.wordWrap": "on",
  "editor.wordWrapColumn": 80,
  "explorer.confirmDelete": false,
  "explorer.decorations.badges": false,
  "extensions.ignoreRecommendations": true,
  "files.associations": {
    ".babelrc": "jsonc",
    ".eslintrc": "jsonc",
    "*.pyi": "python"
  },
  "files.autoSave": "onFocusChange",
  "files.exclude": {
    "**/.DS_Store": true,
    "**/__pycache__": true,
    "**/.cache": true,
    "**/.idea": true,
    "**/.pytest_cache": true,
    "**/.mypy_cache": true,
    "**/.venv": true,
    "**/.coverage": true,
    "**/*.py[co]": true
  },
  "git.autofetch": false,
  "python.formatting.provider": "none",
  "python.pythonPath": "$HOME/.pyenv/shims/python3",
  "python.terminal.activateEnvironment": true,
  "python.workspaceSymbols.enabled": false,
  "telemetry.enableCrashReporter": false,
  "telemetry.enableTelemetry": false,
  "terminal.integrated.env.osx": {
    "SOBOLE_THEME_MODE": "dark"
  },
  "terminal.integrated.enableBell": true,
  "window.clickThroughInactive": false,
  "window.newWindowDimensions": "maximized",
  "window.zoomLevel": 0,
  "workbench.colorCustomizations": {
    "gitDecoration.modifiedResourceForeground": "#d4cb68",
    "activityBar.activeBorder": "#e5b358",
    "editorSuggestWidget.highlightForeground": "#e5b358",
    "editorSuggestWidget.selectedBackground": "#1c2a42",
    "activityBar.foreground": "#797d7b",
    "tab.inactiveForeground": "#676b6a",
    "sideBar.foreground": "#565958",
  },
  "workbench.colorTheme": "Monokai Dimmed",
  "workbench.editor.labelFormat": "short",
  "workbench.iconTheme": "ayu",
  "workbench.startupEditor": "newUntitledFile",
  "workbench.statusBar.visible": true,
  "workbench.tree.renderIndentGuides": "none",
  "workbench.tips.enabled": false,
  "python.linting.enabled": false,
  "git.showInlineOpenFileAction": false,
  "git.autorefresh": true,
  "git.decorations.enabled": true,
  "typescript.updateImportsOnFileMove.enabled": "never",
  "javascript.suggest.autoImports": false,
  "typescript.suggest.autoImports": false,
  "vetur.completion.autoImport": false,
  "vetur.experimental.templateInterpolationService": true,
  "breadcrumbs.enabled": false,
  "update.showReleaseNotes": false
}
